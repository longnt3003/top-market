@model IEnumerable<TopMarket.Models.EntityFramework.Category>

@if (Model != null && Model.Any()) {
    <ul class="navbar_menu">
        @foreach (var item in Model) {
            if (item.Alias.Equals("home")) {
                item.Alias = string.Empty;
            } if (string.IsNullOrEmpty(item.Link) == false) {
                item.Alias = item.Link;
            }
            <li><a href="/@item.Alias">@item.Title</a></li>
        }
    </ul>
}